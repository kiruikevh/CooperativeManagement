    <script type="text/javascript">
    $(document).ready(function () {
        $("#postCodes").change(function () {
            $("#cityname").empty();
            $.ajax({
                type: 'POST',
                url: '@Url.Action("SelectedPosta")',
                dataType: 'json',
                data: { id: $("#postCodes").val() },
                success: function (country) {

                    $.each(country, function (i, country) {
                        $("#cityname").append('<option value="'
                                                    + country.Value + '">'
                                                + country.Text + '</option>');
                    });
                },
                error: function (ex) {
                    alert('Failed.' + ex);
                }
            });
            return false;
        })
    });
    </script>
    <script type="text/javascript">
        //this will add * next to required label
        $('input[type=text],input[type=radio],input[type=checkbox],textarea,select,input[type=email],input[type=number]').each(function () {
            var req = $(this).attr('data-val-required');
            var exclude = $(this).attr('data-exclude');
            if (undefined != req && undefined == exclude) {
                var label = $('label[for="' + $(this).attr('id') + '"]');
                var text = label.text();
                if (text.length > 0) {
                    label.append('<span style="color:red"> *</span>');
                }
            }
        });
    </script>
